"LightmappedGeneric"
{
	"%keywords" "tf"
	"$basetexture" "models\props_moonbase\moon_pit_black"
	//"$baseTexture" "models\props_halloween/unlit_black"
	"$bumpmap" "moon\mercury\mercury1_normal"
	"$bumpframe" "0"
	"$envmapcontrast" "0"
	"$envmap" "cubemaps/cubemap_shapes002"
	"$envmaptint" "[ 1.35 1.22 1.1 ]"
